#
#   The chebyquad problem of Powell. Coded by J.L Morales.
#
param    n := 50;       
param one8 := 1/n;
#
param xinit{i in 1..n} := i/(n-1);

var x{i in 1..n} := xinit[i];


 minimize f:
              ( one8*sum{j in 1..n}( 2*x[j] - 1 ) )^2 
           +
              ( one8*sum{j in 1..n}( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) -
               1 ) + 1/3 )^2
           +
              ( one8*sum{j in 1..n}( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) )^2
           +
              ( one8*sum{j in 1..n} ( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )  + 1/15)^2
           +
              ( one8*sum{j in 1..n}( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) ) )^2
           + 
              ( 
               one8*sum{j in 1..n} 
               ( 
               2*( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) )*( 2*x[j] - 1 ) -   ( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) ) 
               ) 
               + 1/35 
               )^2
            +
              ( 
               one8*sum{j in 1..n} 
               ( 
               2*( 2*( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) ) )* ( 2*x[j] - 1 ) - ( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) ) 
               ) 
               )^2
            + 
               (one8*sum{j in 1..n} ( 2* ( 
               2*( 2*( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) ) )* ( 2*x[j] - 1 ) - ( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) ) 
               )*( 2*x[j] - 1 ) - ( 
               2*( 2*( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) ) )*( 2*x[j] - 1 ) -   ( 2*( 2*( 2*( 2*x[j] - 1 )*( 2*x[j] - 1 ) - 1 )*( 2*x[j] -
               1 ) - ( 2*x[j] - 1 ) )*( 2*x[j] - 1 ) - ( 2*( 2*x[j] -
               1 )*( 2*x[j] - 1 ) - 1 ) ) 
               ) ) + 1/63 
               )^2;

data;
#param xinit :=  1  0.11111111   
#                2  0.22222222
#                3  0.33333333
#                4  0.44444444
#                5  0.55555555
#                6  0.66666665
#                7  0.77777777
#                8  0.88888888;

 
                                                                                             
 display f; display x;
